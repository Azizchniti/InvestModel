{
  "version": 2,
  "builds": [{ "src": "app.py", "use": "@now/python" }],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/predict", "dest": "app.py" },
    { "src": "/(.*)", "dest": "app.py" }
  ],
  "functions": {
    "lambda_function.py": {
      "runtime": "python3.10" 
    }
  }
}
