{
    "version": 2,
    "functions": {
        "api/app.py": {
          "runtime": "python3.9"  
        }
      },
    "builds": [{ "src": "app.py", "use": "@now/python" }],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/predict", "dest": "app.py" },
      { "src": "/(.*)", "dest": "app.py" }
    ]
  }
  