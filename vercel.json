{
    "version": 2,
    "builds": [{ "src": "lambda_function.py", "use": "@now/python" }],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/predict", "dest": "lambda_function.py" },
      { "src": "/(.*)", "dest": "lambda_function.py" }
    ],
    "functions": {
      "lambda_function.py": {
        "runtime": "python3.7"
      }
    }
  }
  